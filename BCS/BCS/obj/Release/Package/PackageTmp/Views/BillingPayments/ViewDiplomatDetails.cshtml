<script>
    function EditDiplomat(item) {
        var p = document.getElementById(item).getElementsByClassName('cell');
        for (var i = 0; i < p.length; i++) {
            var childId = p[i].title;
            var childValue = p[i].innerHTML;
            //alert("Child " + childId + "=" + childValue);
            var elementToPopulate = document.getElementById(childId);
            //alert(elementToPopulate.value);
            elementToPopulate.value = childValue;
        }
    }

    // --- 10/20/2016 --- //
    function AutoCompute(val) {
        if (val.value != null) {
            var AccountT;
            var QuantiT;
            var Start;
            var EndT;
            var EditEndT;
            AccountT = document.getElementById("JVAccountTag").value;
            QuantiT = document.getElementById("Quantity").value;
            Start = document.getElementById("Start").value;
            EditStart = document.getElementById("EditStart").value;
            if (AccountT == "AF") {
                QuantiT = QuantiT * 25;
                EndT = parseInt(Start) + parseInt(QuantiT);
                EndT = EndT - 1;
                EditEndT = parseInt(EditStart) + parseInt(QuantiT);
                EditEndT = EditEndT - 1;
            }
            document.getElementById("End").value = EndT;
            document.getElementById("EditEnd").value = EditEndT;
        }
    }
    // ----------------- //

